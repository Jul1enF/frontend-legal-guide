{
  "expo": {
    "name": "Me Baudelin",
    "slug": "me-baudelin",
    "scheme": "me-baudelin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-font",
      {
        "fonts": ["./assets/fonts/BarlowCondensed-Bold.ttf", "./assets/fonts/BarlowCondensed-ExtraLight.ttf", "./assets/fonts/BarlowCondensed-Light.ttf", "./assets/fonts/BarlowCondensed-Medium.ttf", "./assets/fonts/BarlowCondensed-Regular.ttf",  "./assets/fonts/BarlowCondensed-SemiBold.ttf"]
      }]
    ]
  }
}
